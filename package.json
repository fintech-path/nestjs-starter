{
  "name": "nestjs-starter",
  "version": "0.1.0",
  "description": "This is nestjs starter project.",
  "author": "hcxc",
  "private": false,
  "license": "Apache 2.0",
  "scripts": {
    "gen-gql": "ts-node src/modules/graphql/gen.graphql.typings",
    "prisma-migrate:dev": "env-cmd -f .env.dev.json npx prisma migrate dev --name init --schema=./src/common/database/schema.prisma",
    "prisma-migrate:prod": "env-cmd -f .env.dev.json npx prisma migrate deploy --name init --schema=./src/common/database/schema.prisma",
    "gen-prisma:dev": "env-cmd -f .env.dev.json npx prisma generate --schema=./src/common/database/schema.prisma",
    "gen-prisma:prod": "env-cmd -f .env.prod.json npx prisma generate --schema=./src/common/database/schema.prisma",
    "prisma-studio": "pnpm run prisma-studio:dev",
    "prisma-studio:dev": "env-cmd -f .env.dev.json npx prisma studio --schema=./src/common/database/schema.prisma",
    "prisma-studio:prod": "env-cmd -f .env.prod.json npx prisma studio --schema=./src/common/database/schema.prisma",
    "prebuild": "rimraf dist",
    "build": "pnpm run prebuild && pnpm run gen-prisma:prod && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prod": "pnpm run gen-prisma:prod && env-cmd -f .env.prod.json node dist/main",
    "start": "pnpm run prod",
    "dev": "pnpm run gen-prisma:dev && env-cmd -f .env.dev.json nest start --watch",
    "debug": "pnpm run gen-prisma:dev && env-cmd -f .env.dev.json nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm run gen-prisma:dev && jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@graphql-tools/utils": "^8.13.1",
    "@nestjs/apollo": "^10.1.4",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.4",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.5.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/multer": "^1.4.7",
    "@willsoto/nestjs-prometheus": "^5.1.1",
    "apollo-server": "^3.10.4",
    "apollo-server-express": "^3.10.4",
    "apollo-server-plugin-base": "^3.6.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.2",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "env-cmd": "^10.1.0",
    "express-rate-limit": "^6.7.0",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "helmet": "^5.1.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.9.1",
    "nestjs-rate-limiter": "^3.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.3.0",
    "prisma": "^4.5.0",
    "prom-client": "^14.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "winston": "^3.8.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
